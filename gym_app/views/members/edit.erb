<h1>Edit Member</h1>
<form action="/gym/members/<%=@member.id%>" method="post">
  <label for="first-name">First Name: </label>
  <input type="text" id="first-name" name="first_name" value="<%= @member.first_name%>">
  <br>

  <label for="last-name">Last Name: </label>
  <input type="text" id="last-name" name="last_name" value="<%= @member.last_name %>">
  <br>

  <label for="session">Session: </label>
  <select id="session" name="session">
    <% for session in @sessions %>
    <option value= "<%=session.id%>"><%=session.type%></option>
    <% end %>
  </select>
  <br>

  <label for="age">Age: </label>
  <input type="number" id= "age" name="age" value="<%= @member.age%>">
  <br>
  <input type="submit" value="Submit application">
</form>
