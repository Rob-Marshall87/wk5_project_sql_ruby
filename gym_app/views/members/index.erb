<h1>All Gym Members</h1>

<div id="member-wrapper">
<% for member in @members %>

<div class="member">
  <p>First name: <%= member.first_name  %></p>
  <p>Last name: <%= member.last_name %></p>
  <p>Session: <%= Session.find_by_id(member.session).type %></p>
  <p>Age: <%= member.age  %></p>
  <p><a href="/gym/members/<%= member.id %>">More Options</a></p>
  </div>

  <br>
<% end %>
</div>
